      "id": "cmd-12",
      "comment": "Verificar botão Adicionar",
      "command": "assertElementPresent",
      "target": "linkText=Adicionar",
      "targets": [],
      "value": ""
    }, {
      "id": "cmd-13",
      "comment": "Verificar texto do botão Adicionar",
      "command": "assertText",
      "target": "linkText=Adicionar",
      "targets": [],
      "value": "Adicionar"
    }, {
      "id": "cmd-14",
      "comment": "Verificar classe do botão Adicionar",
      "command": "assertAttribute",
      "target": "linkText=Adicionar@class",
      "targets": [],
      "value": "*btn-dark*"
    }, {
      "id": "cmd-15",
      "comment": "Verificar botão Consultar",
      "command": "assertElementPresent",
      "target": "linkText=Consultar",
      "targets": [],
      "value": ""
    }, {
      "id": "cmd-16",
      "comment": "Verificar texto do botão Consultar",
      "command": "assertText",
      "target": "linkText=Consultar",
      "targets": [],
      "value": "Consultar"
