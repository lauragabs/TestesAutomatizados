{
  "id": "listar-veterinario",
  "version": "2.0",
  "name": "Listar Veterinário",
  "url": "http://localhost:8080",
  "tests": [{
    "id": "test-listar-veterinarios",
    "name": "Verificar listagem de veterinários",
    "commands": [{
      "id": "cmd-1",
      "comment": "Cenário: Verificar estrutura e conteúdo da listagem",
      "command": "open",
      "target": "/home",
      "targets": [],
      "value": ""
    }, {
      "id": "cmd-2",
      "comment": "Aguardar carregamento da página",
      "command": "waitForElementPresent",
      "target": "css=.table",
      "targets": [],
      "value": "3000"
    }, {
      "id": "cmd-3",
      "comment": "Verificar título da página",
      "command": "assertText",
      "target": "css=h1",
      "targets": [],
      "value": "Veterinarios"
    }, {
      "id": "cmd-4",
      "comment": "Verificar se tabela existe",
      "command": "assertElementPresent",
      "target": "css=.table",
      "targets": [],
      "value": ""
    }, {
      "id": "cmd-5",
      "comment": "Verificar classe da tabela",
      "command": "assertAttribute",
      "target": "css=.table@class",
      "targets": [],
      "value": "*table*"
    }, {
      "id": "cmd-6",
      "comment": "Verificar classe table-light",
      "command": "assertAttribute",
      "target": "css=.table@class",
      "targets": [],
      "value": "*table-light*"
    }, {
      "id": "cmd-7",
      "comment": "Verificar cabeçalho da tabela",
      "command": "assertElementPresent",
      "target": "css=.table-dark",
      "targets": [],
      "value": ""
    }, {
      "id": "cmd-8",
      "comment": "Verificar cabeçalho Nome",
      "command": "assertText",
      "target": "xpath=//th[text()='Nome']",
      "targets": [],
      "value": "Nome"
    }, {
      "id": "cmd-9",
      "comment": "Verificar cabeçalho Especialidade",
      "command": "assertText",
      "target": "xpath=//th[text()='Especialidade']",
      "targets": [],
      "value": "Especialidade"
